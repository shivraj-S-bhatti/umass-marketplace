# Environment variables for UMass Marketplace
# Copy this file to .env and update the values as needed

# Database configuration
POSTGRES_DB=umarket
POSTGRES_USER=umarket
POSTGRES_PASSWORD=umarket

# Spring Boot API configuration
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/umarket
SPRING_DATASOURCE_USERNAME=umarket
SPRING_DATASOURCE_PASSWORD=umarket
SPRING_PROFILES_ACTIVE=dev

# OAuth2 configuration (placeholders - add your actual values)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Frontend configuration
VITE_API_BASE_URL=http://localhost:8080
